<footer class="footer">
    <div class="container">
			<div class="row-fluid bgLG">
			    <div class="span12">
			      <div class="">
			            <div class="row-fluid">
			            <div class="span3">&nbsp;</div>
			            
			              <div class="row-fluid fMenu">
			                <div class="span5"><br />
									      	<ul class="nav">
														<% if get_all_pages.size > 0 %>
														  <% for o_row in get_all_pages %>
																<li><%= link_to o_row.name, "/other/#{o_row.page_route}" %></li>
														  <% end %>
														<% end %>
									        </ul>
			                </div>
			          		</div>
			          <div class="span3">&nbsp;</div>
			        </div>
			        <div class="copyRightText"><%=raw t("general.footer_msg") %></div>
			    </div>
			    
			  </div><!--/span-->
			</div><!--/row-->	            
    </div>
</footer>   

<div id="spinner" style="margin: 0px; left: 0px; display: none;" class="loaderimgText">
  <%= image_tag "loader.gif" %><br><div class="backTransparant"></div>
</div>
<script type="text/javascript">
	$(document).ready(function() {
		$("input:text").eq(0).focus();
		$("#spinner").hide();
	});
</script>
